{
  "name": "saldometrobus-app",
  "version": "2.2.0",
  "description": "Saldo Metrobús es un sitio de consultas de saldo y movimientos para las tarjetas de metro y metrobús de Panamá a cualquier hora del día.",
  "author": "Yizack Rangel",
  "license": "MIT",
  "type": "module",
  "homepage": "https://saldometrobus.yizack.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/saldometrobus-app"
  },
  "bugs": {
    "url": "https://github.com/Yizack/saldometrobus-app/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=5173",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "nuxt prepare",
    "android": "npx cap add android",
    "ios": "npx cap add ios",
    "resources": "npx @capacitor/assets generate --android --ios --splashBackgroundColorDark #F8F9FC",
    "versionApp": "node updateVersion.js",
    "trapeze": "npx trapeze run config.yaml -y",
    "clean": "node cleanOutput.js",
    "sync": "npx cap sync",
    "key": "openssl base64 -A -in saldometrobus_key.jks -out saldometrobus_key.jks.txt",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "devDependencies": {
    "@capacitor-community/sqlite": "^4.6.3-4",
    "@capacitor/android": "^4.7.3",
    "@capacitor/app": "^4.1.1",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^4.7.3",
    "@capacitor/core": "^4.7.3",
    "@capacitor/dialog": "^4.1.0",
    "@capacitor/ios": "^4.7.3",
    "@capacitor/preferences": "^4.0.2",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@capacitor/toast": "^4.1.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@trapezedev/configure": "^7.0.8",
    "eslint": "^8.37.0",
    "jeep-sqlite": "^2.3.3",
    "nuxt": "^3.3.3",
    "nuxt-icon": "^0.3.3",
    "stylelint": "^15.4.0",
    "stylelint-order": "^6.0.3"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.7",
    "bootstrap": "^5.3.0-alpha2",
    "chart.js": "^4.2.1"
  },
  "overrides": {
    "vue": "latest"
  }
}
