{
  "extends": "stylelint-config-standard",
  "rules": {
    "length-zero-no-unit": true,
    "property-no-vendor-prefix": null,
    "rule-empty-line-before": ["always-multi-line", {"except": ["first-nested"]}]
  },
  "ignoreFiles": [
    "android/**/*",
    "node_modules/**/*",
    ".nuxt/**/*",
    "dist/**/*",
    ".output/**/*"
  ]
}
